        "resolve-url-loader": "^2.3.1",
        "sass": "^1.20.1",
        "sass-loader": "7.*",
        "tailwindcss": "^1.2",
        "vue": "^2.5.17",
        "vue-template-compiler": "^2.6.10",
        "webpack-bundle-analyzer": "^3.6.0"
    }
